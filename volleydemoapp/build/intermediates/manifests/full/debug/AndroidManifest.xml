<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mani.volleydemo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="com.mani.volleydemo.app.VolleySampleApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.mani.volleydemo.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.mani.volleydemo.JSONObjectRequestActvity"
            android:label="@string/app_name" />
        <activity
            android:name="com.mani.volleydemo.StringObjectRequestActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.mani.volleydemo.GSONObjectRequestActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.mani.volleydemo.SSLConnectionActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.mani.volleydemo.NetworkImageActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.mani.volleydemo.MultiPartRequestActivity"
            android:label="@string/app_name" />
    </application>

</manifest>